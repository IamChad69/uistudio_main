{
  "name": "uiscraper-extension",
  "version": "1.0.0",
  "description": "Cross-browser web scraping tool for extracting UI elements and data from websites",
  "private": true,
  "repository": "https://github.com/uiScraper/uiScraper",
  "author": {
    "name": "uiScraper Team",
